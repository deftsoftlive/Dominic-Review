    left: 0;    
    background: url("data:image/svg+xml;base64,");
    background-repeat: no-repeat;
    background-size: 0.5rem;
    background-position: 50%;
    transform: rotate(180deg);
}
.ui-datepicker-header a.ui-datepicker-next {
    right: 0;
    background: url('data:image/svg+xml;base64,');
    background-repeat: no-repeat;
    background-size: 10px;
    background-position: 50%;
}
